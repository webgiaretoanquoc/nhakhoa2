 ng\u00e0y nay,\u00a0ti\u1ebfng Anh th\u01b0\u01a1ng m\u1ea1i\u00a0tr\u1edf th\u00e0nh m\u1ed9t ph\u1ea7n quan tr\u1ecdng th\u00ec m\u1ed9t c\u00e1i &#8230;<\/p>\n","protected":false},"author":1,"featured_media":632,"comment_status":"open","ping_status":"open","sticky":false,"template":"","format":"standard","meta":[],"categories":[1],"tags":[],"_links":{"self":[{"href":"https:\/\/nhakhoa2.webtudong8giay.com\/wp-json\/wp\/v2\/posts\/631"}],"collection":[{"href":"https:\/\/nhakhoa2.webtudong8giay.com\/wp-json\/wp\/v2\/posts"}],"about":[{"href":"https:\/\/nhakhoa2.webtudong8giay.com\/wp-json\/wp\/v2\/types\/post"}],"author":[{"embeddable":true,"href":"https:\/\/nhakhoa2.webtudong8giay.com\/wp-json\/wp\/v2\/users\/1"}],"replies":[{"embeddable":true,"href":"https:\/\/nhakhoa2.webtudong8giay.com\/wp-json\/wp\/v2\/comments?post=631"}],"version-history":[{"count":1,"href":"https:\/\/nhakhoa2.webtudong8giay.com\/wp-json\/wp\/v2\/posts\/631\/revisions"}],"predecessor-version":[{"id":633,"href":"https:\/\/nhakhoa2.webtudong8giay.com\/wp-json\/wp\/v2\/posts\/631\/revisions\/633"}],"wp:featuredmedia":[{"embeddable":true,"href":"https:\/\/nhakhoa2.webtudong8giay.com\/wp-json\/wp\/v2\/media\/632"}],"wp:attachment":[{"href":"https:\/\/nhakhoa2.webtudong8giay.com\/wp-json\/wp\/v2\/media?parent=631"}],"wp:term":[{"taxonomy":"category","embeddable":true,"href":"https:\/\/nhakhoa2.webtudong8giay.com\/wp-json\/wp\/v2\/categories?post=631"},{"taxonomy":"post_tag","embeddable":true,"href":"https:\/\/nhakhoa2.webtudong8giay.com\/wp-json\/wp\/v2\/tags?post=631"}],"curies":[{"name":"wp","href":"https:\/\/api.w.org\/{rel}","templated":true}]}}